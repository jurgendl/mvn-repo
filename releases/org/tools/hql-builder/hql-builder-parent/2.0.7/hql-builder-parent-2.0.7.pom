	<name>hql-builder-parent</name>
	<description>HqlBuilder</description>
	<url>http://hql-builder.googlecode.com</url>
	<properties>
		<deploy.skip.demo>true</deploy.skip.demo>
		<deploy.skip.web>true</deploy.skip.web>
		<!-- general properties -->
		<targetJdk>1.7</targetJdk><!-- should be compatible with 1.6; not ready for 1.8 - reason library problems -->
		<project.build.sourceEncoding>utf-8</project.build.sourceEncoding><!-- fixed encoding -->
		<project.reporting.outputEncoding>${project.build.sourceEncoding}</project.reporting.outputEncoding>
		<!--  -->
		<maven.javadoc.skip>true</maven.javadoc.skip><!-- skip by default -->
		<!-- maven-site-plugin -->
		<maven.site.skip>false</maven.site.skip><!-- do not skip by default -->
		<!-- maven-compiler-plugin -->
		<maven.compiler.optimize>true</maven.compiler.optimize>
		<maven.compiler.fork>true</maven.compiler.fork>
		<maven.compiler.source>${targetJdk}</maven.compiler.source>
		<maven.compiler.target>${targetJdk}</maven.compiler.target>
		<maven.compiler.verbose>true</maven.compiler.verbose>
		<maven.compiler.debug>true</maven.compiler.debug>
		<maven.compiler.debuglevel>lines,vars,source</maven.compiler.debuglevel>
		<!-- maven-surefire-plugin -->
		<maven.test.failure.ignore>true</maven.test.failure.ignore>
		<!-- versions checked 2014.05.07 and tested against 4.2 -->
		<version.hibernate>4.2</version.hibernate><!-- version 4.3 is not finished -->
		<version.slf4j>1.7.7</version.slf4j><!-- recent version, keep up to date -->
		<version.logback>1.1.2</version.logback><!-- recent version, keep up to date -->
		<version.logback.spring>0.1.2</version.logback.spring>
		<version.spring>3.2.11.RELEASE</version.spring><!-- keep up to date 3.x.x, v 4.0.3.RELEASE Java 1.8 ready but breaks RestEasy -->
		<version.swingeasy>3.0.0.4</version.swingeasy><!-- recent version, keep up to date -->
		<version.lucene>4.9.0</version.lucene><!-- keep up to date 4.x.x -->
		<version.groovy>2.3.1</version.groovy><!-- Java 1.8 ready, recent version, keep up to date -->
		<version.joda>2.3</version.joda><!-- maybe one day replaced by Java 1.8 together with Date/Calender -->
		<version.l2fprod>7.3</version.l2fprod><!-- not being updated -->
		<version.commons-codec>1.9</version.commons-codec><!-- recent version, keep up to date -->
		<version.commons-io>2.4</version.commons-io><!-- not being updated, keep up to date -->
		<version.commons-lang>2.6</version.commons-lang><!-- not being updated -->
		<version.commons-lang3>3.3.1</version.commons-lang3><!-- recent version, keep up to date -->
		<version.commons-collections>3.2.1</version.commons-collections><!-- not being updated -->
		<version.commons-collections4>4.0</version.commons-collections4><!-- recent version, keep up to date -->
		<version.commons-beanutils>1.9.1</version.commons-beanutils><!-- recent version, keep up to date -->
		<version.javassist>3.18.1-GA</version.javassist><!-- seems to be Java 1.8 ready -->
		<version.ehcache>2.8.2</version.ehcache><!-- recent version, keep up to date -->
		<version.resteasy>3.0.8.Final</version.resteasy><!-- keep up to date 3.x.x -->
