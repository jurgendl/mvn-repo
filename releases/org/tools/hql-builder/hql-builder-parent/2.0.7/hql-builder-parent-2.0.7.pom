		<version.junit>4.11</version.junit><!-- recent version, keep up to date -->
		<version.bouncycastle>1.51</version.bouncycastle><!-- always use the latest version, available as of 2013-12-03 -->
		<version.lambdaj>2.4</version.lambdaj><!-- maybe one day replaced by Java 1.8 -->
		<version.cglib>3.1</version.cglib><!-- check if Java 1.8 ready, mostly dead project, Spring includes fork -->
		<version.asm>5.0.2</version.asm><!-- Java 1.8 ready, Spring includes version for included cglib -->
		<version.activation>1.1.1</version.activation><!-- not being updated -->
